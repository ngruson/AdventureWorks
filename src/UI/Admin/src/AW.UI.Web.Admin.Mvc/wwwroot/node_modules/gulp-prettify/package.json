{
  "name": "gulp-prettify",
  "description": "Prettify, format, beautify HTML.",
  "version": "0.5.0",
  "homepage": "https://github.com/jonschlinkert/gulp-prettify",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-prettify",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-prettify/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE"
  ],
  "main": "index.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "js-beautify": "^1.6.3",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-format-md": "^0.1.10",
    "gulp-util": "^3.0.7",
    "mocha": "^3.0.0"
  },
  "keywords": [
    "beautify",
    "code",
    "format",
    "gulp",
    "gulpplugin",
    "html",
    "indent",
    "js-beautify",
    "plugin",
    "prettify"
  ],
  "verb": {
    "related": {
      "list": [
        "gulp-dest",
        "gulp-extname",
        "gulp-htmlmin"
      ]
    },
    "plugins": [
      "gulp-format-md"
    ],
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
